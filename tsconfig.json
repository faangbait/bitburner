{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ES2020",
    "rootDir": "src/",
    "baseUrl": "src/",
    "paths": {
      "lib/*": [
        "lib/*",
      ],
      "logic/*": [
        "logic/*",
      ],
      "modules/*": [
        "modules/*",
      ],
      "sys/*": [
        "sys/*",
      ],
      "coding_contracts/*": [
        "coding_contracts/*",
      ],
    },
    "outDir": "build/",
    "moduleResolution": "node",
    "strictNullChecks": true,
    "strictPropertyInitialization": true
  }
}
